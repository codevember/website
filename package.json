{
  "name": "codevember-website",
  "version": "1.0.0",
  "description": "Codevember 2016 website",
  "main": "index.js",
  "scripts": {
    "start": "npm run server & npm run dev",
    "dev": "webpack -d --watch",
    "server": "webpack-dev-server --content-base public/ --history-api-fallback",
    "prod": "npm run clean && webpack -p --progress --optimize-minimize --optimize-dedupe",
    "clean": "find ./public/build -name \"*.map\" -type f -delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codevember/website.git"
  },
  "author": "Valentin Daguenet <valentin.daguenet@gmail.com> (http://vdaguenet.fr/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codevember/website/issues"
  },
  "homepage": "https://github.com/codevember/website#readme",
  "devDependencies": {
    "autoprefixer-stylus": "^0.10.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "brindille-stylus-extends": "0.0.2",
    "express": "^4.14.0",
    "firebase": "^3.4.1",
    "stylus-easing": "^1.0.0",
    "stylus-media-queries": "^1.0.0",
    "stylus-reset": "0.0.1",
    "vue": "^2.0.1",
    "vue-router": "^2.0.0"
  }
}
